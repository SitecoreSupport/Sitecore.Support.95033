<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <commands>
      <command name="webedit:newrendering" set:type="Sitecore.Support.NewRendering,Sitecore.Support.95033"/>
    </commands>
    <sitecore.experienceeditor.speak.requests>
      <request name="ExperienceEditor.CanAddComponent" 
               type="Sitecore.Support.ExperienceEditor.Speak.Ribbon.Requests.AddRendering.CanAddRendering, Sitecore.Support.95033"
               patch:instead="request[@name='ExperienceEditor.CanAddComponent']"/>
      <request name="ExperienceEditor.EnableEditing.CanEdit" 
               type="Sitecore.Support.ExperienceEditor.Speak.Ribbon.Requests.EnableEditing.CanEdit, Sitecore.Support.95033"
               patch:instead="request[@name='ExperienceEditor.EnableEditing.CanEdit']"/>
      <request name="ExperienceEditor.EnableDesigning.CanDesign" 
               type="Sitecore.Support.ExperienceEditor.Speak.Ribbon.Requests.EnableDesigning.CanDesign, Sitecore.Support.95033"
               patch:instead="request[@name='ExperienceEditor.EnableDesigning.CanDesign']"/>

    </sitecore.experienceeditor.speak.requests>
  </sitecore>
</configuration>